generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model User {
  id Int @id @default(autoincrement())
  email String  @unique
  phoneNum String  @unique
  name String?
  createdAt DateTime @default(now())
}